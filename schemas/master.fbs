namespace Ecosystem;

enum Gender : byte { Male = 1, Female = 0}

table Master {
    name:string (key, required);
    kind:string (required);
    chromosome:string (required);
    generation:ulong = 1;
    immunity:double = 0.0;
    gender:Gender = Male;
    age:uint = 0;
    height:double = 0.0;
    weight:double = 0.0;
    static_fitness:double = 0.0;
}

root_type Master;